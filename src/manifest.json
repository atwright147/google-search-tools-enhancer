{
    "manifest_version": 2,
    "name": "Google Search Tools Time Crx",
    "description": "Improve Google Search Tools better",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "*://*.google.com/search*",
                "*://*.google.co.uk/search*"
            ],
            "js": [
                "scripts/content-script.js"
            ],
            "run_at": "document_idle"
        }
    ]
}
